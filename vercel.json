{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }

    ],

    "regions": ["hnd1"],

    "functions": {

        "api/*.py": {

            "memory": 1024,

            "maxDuration": 10

        }

    }

} 
